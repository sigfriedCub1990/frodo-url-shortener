{
  "name": "frodo-url-shortener",
  "version": "1.0.0",
  "description": "Frodo, the URL shortener.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:build npm:start-server ",
    "build": "npx tsc",
    "start-server": "node dist/server.js",
    "dev": "concurrently npm:watch*",
    "watch": "nodemon -q dist/server.js",
    "watch-ts": "npx tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "joi": "^17.7.1",
    "murmurhash3": "^0.5.0",
    "redis": "^4.6.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "concurrently": "^7.6.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^29.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "url-shortener",
    "murmurhash3"
  ],
  "author": "sigfried",
  "license": "MIT"
}
